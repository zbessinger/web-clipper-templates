{
	"schemaVersion": "0.1.0",
	"name": "fullHtml",
	"behavior": "create",
	"noteContentFormat": "\n#webclip/unread\n\n# {{title}}\n\n{{fullHtml|markdown}}",
	"properties": [
		{
			"name": "created",
			"value": "{{time|date:\\\"YYYY-MM-DDTHH:mm:ssZ\\\"}}",
			"type": "date"
		},
		{
			"name": "reviewed",
			"value": "",
			"type": "text"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "title",
			"value": "{{title}}",
			"type": "text"
		},
		{
			"name": "author",
			"value": "{{author|split:\\\",\\\"|join}}",
			"type": "multitext"
		},
		{
			"name": "published",
			"value": "{{published|date:\\\"YYYY-MM-DD\\\"}}",
			"type": "date"
		},
		{
			"name": "description",
			"value": "{{description}}",
			"type": "text"
		}
	],
	"triggers": [],
	"noteNameFormat": "{{published|date:\"YYYY-MM-DD\"}} WEB {{title|safe_name}}",
	"path": "webclips"
}