{
	"schemaVersion": "0.1.0",
	"name": "VMware Blog",
	"behavior": "create",
	"noteContentFormat": "\n#webclip/unread\n\n# {{title}}\n\n{{selectorHtml:div.article-body|markdown}}",
	"properties": [
		{
			"name": "created",
			"value": "{{time|date:\\\"YYYY-MM-DDTHH:mm:ssZ\\\"}}",
			"type": "date"
		},
		{
			"name": "reviewed",
			"value": "",
			"type": "text"
		},
		{
			"name": "url",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "title",
			"value": "{{title}}",
			"type": "text"
		},
		{
			"name": "author",
			"value": "{{author|split:\\\",\\\"|join}}",
			"type": "multitext"
		},
		{
			"name": "published",
			"value": "{{selector:div[class=\\\"blog-date\\\"]|date:\\\"YYYY-MM-DD\\\"}}",
			"type": "date"
		},
		{
			"name": "description",
			"value": "{{description}}",
			"type": "text"
		}
	],
	"triggers": [
		"https://core.vmware.com/blog"
	],
	"noteNameFormat": "{{selector:div[class=\"blog-date\"]|date:\"YYYY-MM-DD\"}} WEB {{title|safe_name}}",
	"path": "webclips"
}